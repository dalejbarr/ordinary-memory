    issn = {00243841},
    journal = {Lingua},
    keywords = {drr0, language\_comprehension, language\_production, overspecification},
    month = aug,
    number = {8},
    pages = {1956--1972},
    posted-at = {2010-11-03 16:44:11},
    priority = {2},
    title = {{Over-informative children: Production/comprehension asymmetry or tolerance to pragmatic violations?}},
    url = {http://dx.doi.org/10.1016/j.lingua.2010.02.005},
    volume = {120},
    year = {2010}
}

@article{sonnenschein84,
    abstract = {{Much of our speech is redundant in the sense that we say more than is minimally necessary to be informative. However, little is known about the effects of verbal redundancy on listeners' responses. 3 studies investigated whether, and under what conditions, redundant verbal information would facilitate a listener's performance. Kindergartners, first graders, and fourth graders had to select which of several groups of pictures a message described. The messages were either redundant-mentioning more than minimally necessary to be informative-or contrastive-mentioning the minimum necessary to be informative. The results indicated that when the pictorial array was too difficult, due either to the size of the stimuli or the length of exposure, for the listeners to process everything, verbal redundancy facilitated the older children's performance by increasing the chances that they would notice 1 of the items mentioned. Redundant information never helped, and, in fact, even had a debilitating effect relative to contrastive messages on the younger children's performance. The younger children appeared both to have faulty recall and to use a different decision criterion relative to the older children. The data are discussed in terms of the implications for theories of communication development.}},
    author = {Sonnenschein, Susan},
    citeulike-article-id = {8185670},
    citeulike-linkout-0 = {http://dx.doi.org/10.2307/1129385},
    citeulike-linkout-1 = {http://www.jstor.org/stable/1129385},
