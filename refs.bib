    keywords = {disfluency, pragmatics, psycholinguistics, visual\_world},
    number = {5},
    pages = {706--727},
    posted-at = {2010-06-02 12:21:21},
    priority = {4},
    publisher = {Psychology Press},
    title = {{Making predictions from speech with repairs: Evidence from eye movements}},
