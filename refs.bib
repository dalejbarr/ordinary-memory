    keywords = {p-curve},
    number = {1},
    pages = {30--37},
    posted-at = {2014-02-12 13:25:19},
    priority = {2},
    title = {{Embodied Terror Management: Interpersonal Touch Alleviates Existential Concerns Among Individuals With Low Self-Esteem}},
    url = {http://pss.sagepub.com/cgi/content/abstract/25/1/30},
    volume = {25},
