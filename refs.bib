    keywords = {eye\_gaze, visual\_attention, visual\_search},
    number = {01},
    pages = {87--114},
    posted-at = {2011-10-20 23:53:12},
    priority = {2},
    title = {{The magical number 4 in short-term memory: A reconsideration of mental storage capacity}},
    url = {http://dx.doi.org/10.1017/s0140525x01003922},
    volume = {24},
    year = {2001}
}

